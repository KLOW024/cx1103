//random number in C


#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main {
    srand(time(NULL));
    n = (rand() % 100);        //%100 for no. from 0 to 99
    return 0;
}
   
size_t strlen(char * s) {     //for removing '\0' when counting strlen()
    char *p = s;
    for (; *p != '\0'; p++) {}
    return p - s;
}
